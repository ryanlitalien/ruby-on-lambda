{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::${AWS::AccountId}:role/QuickTrackStack-QuickTrackLambdaRole-VV78RYECHQXV"
      },
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::www.quicktrack.pro",
        "arn:aws:s3:::www.quicktrack.pro/*"
      ]
    },
    {
      "Sid": "PublicReadGetObject",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:Get*",
        "s3:List*"
      ],
      "Resource": [
        "arn:aws:s3:::www.quicktrack.pro",
        "arn:aws:s3:::www.quicktrack.pro/*"
      ]
    }
  ]
}
